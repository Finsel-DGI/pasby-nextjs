{
  "name": "@finsel-dgi/pasby-auth-nextjs",
  "version": "1.0.0",
  "description": "Next.js authentication with pasby",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/cjs/index.js",
      "types": "./dist/types/server/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "npm run rm && rollup -c",
    "rm": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Finsel-DGI/pasby-nextjs.git"
  },
  "keywords": [
    "typescript",
    "react",
    "react hooks",
    "pasby",
    "nextjs",
    "auth",
    "hook"
  ],
  "author": "cjackmay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Finsel-DGI/pasby-nextjs/issues"
  },
  "homepage": "https://github.com/Finsel-DGI/pasby-nextjs#readme",
  "peerDependencies": {
    "next": "^12.2.5 || ^13 || ^14",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.4",
    "rollup": "^4.21.1",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "axios": "^1.7.5",
    "cookies-next": "^4.2.1"
  }
}